<!DOCTYPE html>
<head>
<title>Webshop</title>
<link rel="stylesheet" href="/static/stylesheets/main.css" type="text/css">
<script src="https://kit.fontawesome.com/8a91947445.js" crossorigin="anonymous"></script>
</head>
<body>

    <ul>
        <li><a href="/home"> Home </a></li>
        <li><a href="/login">Login</a></li>
        <li><a href="/register"> Register</a></li>
        <li><a href="/reset">Reset Password?</a></li>
       <li><a href="/logout"> Logout</a></li>
    </ul>

   <div>

    <h4>UNDER CONSTRUCTION</h4>

    <%  products.forEach ( product => { %>
    <div>
        <%= product.name %>
    </div>
    <div>
        <%= product.price %>
    </div>
    <a href="/addToCart/<%=product.id%>">Add To Cart</a>
    <div>/////////////////**/////////////////</div>
    
    <% }) %> 

   </div>

</body>
</html>